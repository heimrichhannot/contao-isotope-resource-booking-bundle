{
  "name": "heimrichhannot/contao-isotope-resource-booking-bundle",
  "type": "contao-bundle",
  "description": "This bundle adds a booking functionality to Isotope.",
  "license": "LGPL-3.0-or-later",
  "require": {
    "php": "^7.4||^8.0",
    "contao/core-bundle": "^4.9",
    "heimrichhannot/contao-be_explanation-bundle": "^2.4",
    "heimrichhannot/contao-utils-bundle": "^2.200",
    "isotope/isotope-core": "^2.7",
    "symfony/http-kernel": "^4.4||^5.0"
  },
  "conflict": {
    "heimrichhannot/contao-encore-bundle": "<1.5"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\IsotopeResourceBookingBundle\\": "src/"
    }
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\IsotopeResourceBookingBundle\\ContaoManager\\Plugin",
    "foxy": true
  }
}
